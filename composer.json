{
  "name": "mouf/cms.generator",
  "description": "This package allow you to auto generate functional CMS components.",
  "keywords": ["mouf", "cms", "components"],
  "homepage": "https://github.com/thecodingmachine/cms.generator",
  "type": "mouf-library",
  "license": "MIT",
  "authors": [
    {
      "name": "Jean-Baptiste Charron",
      "email": "jb.charron@thecodingmachine.com"
    },
    {
      "name": "Xavier Huberty",
      "email": "x.huberty@thecodingmachine.com"
    },
    {
      "name": "Thomas Carli",
      "email": "t.carli@thecodingmachine.com"
    }
  ],

  "require": {
    "mouf/mvc.splash": "~7.0",
    "mouf/database.tdbm": "^4.0.0-beta15",
    "mouf/database.patcher": "~2.0",
    "mouf/html.htmlelement": "~2.0",
    "mouf/html.widgets.evolugrid": "~5.0",
    "fabpot/php-cs-fixer": "~1.11"
  },
  "autoload": {
    "psr-4": {"Mouf\\Cms\\Generator\\": "src/"}
  },
  "extra": {
    "mouf": {
      "install": [
        {
          "type": "file",
          "file": "src/install.php",
          "scope": "global",
          "description": "Installing CMS Generator"
        }
      ],
      "require-admin": [
        "src/CmsGeneratorAdmin.php"
      ]
    }
  },

  "minimum-stability": "dev",
  "prefer-stable": true
}