{
  "name": "mouf/cms.scaffolder",
  "description": "This package allow you to auto generate functional CMS components.",
  "keywords": ["mouf", "cms", "scaffold"],
  "homepage": "https://github.com/thecodingmachine/cms.scaffolder",
  "type": "mouf-library",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas Carli",
      "email": "t.carli@thecodingmachine.com"
    },
    {
      "name": "Jean-Baptiste Charron",
      "email": "jb.charron@thecodingmachine.com"
    },
    {
      "name": "Xavier Huberty",
      "email": "x.huberty@thecodingmachine.com"
    }
  ],

  "require": {
    "mouf/mvc.splash": "~7.0",
    "mouf/database.tdbm": "^4.0.0-beta15",
    "mouf/database.patcher": "~2.0",
    "mouf/html.htmlelement": "~2.0",
    "mouf/codestylefixservice": "~1.0",
    "symfony/filesystem": "~2.0 | ~3.0",
    "mouf/security.rightsservice-splash": "~4.0"
  },
  "autoload": {
    "psr-4": {"Mouf\\Cms\\Scaffolder\\": "src/"}
  },
  "extra": {
    "mouf": {
      "require-admin": [
        "src/CmsScaffolderAdmin.php"
      ]
    }
  },

  "minimum-stability": "dev",
  "prefer-stable": true
}
